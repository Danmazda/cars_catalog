<%- include("./partials/header") -%>
<main>
  <section id="cards">
    <% cars.forEach( car=>{ %>
    <article class="carCard" key="<%= car.id %> ">
      <div class="flex-row">
        <p><%= car.cardId %></p>
        <p><%= car.name %></p>
      </div>
      <img src="<%= car.img %> " alt="<%= car.name %> photo" />
      <div class="flex-row">
        <p>0-100</p>
        <p><%= car.toOneHundred %> s</p>
      </div>
      <div class="flex-row">
        <p>Engine Capacity</p>
        <p><%= car.engineCapacity %> cmÂ³</p>
      </div>
      <div class="flex-row">
        <p>Horse Power</p>
        <p><%= car.hp %> HP</p>
      </div>
    </article>
    <% }); %>
  </section>
</main>
<%- include("./partials/footer") -%>
